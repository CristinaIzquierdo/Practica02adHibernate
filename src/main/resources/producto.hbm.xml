<?xml version = "1.0" encoding = "utf-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="model.Producto" table="producto">
        <id name="codigoProducto" column="codigo_producto"></id>
		<property name="nombre">
		    <column name="nombre"/>
		</property>
		<property name="gama">
		    <column name="gama"/>
		</property>
		<property name="dimensiones">
		    <column name="dimensiones"/>
		</property>
		<property name="proveedor">
		    <column name="proveedor"/>
		</property>
		<property name="descripcion">
		    <column name="descripcion"/>
		</property>
		<property name="cantidadStock">
		    <column name="cantidad_en_stock"/>
		</property>
		<property name="precioVenta">
		    <column name="precio_venta"/>
		</property>
		<property name="precioProveedor">
		    <column name="precio_proveedor"/>
		</property>
    </class>
</hibernate-mapping>